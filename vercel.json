{
    "builds": [
        {
            "src": "server/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "web/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/evaluate",
            "dest": "/server/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "/web/$1"
        }
    ]
}